# Top-level filter (only include the following...)
^(?!(?:script|examples|lib|bin|t|xt|maint|sql|root|dbicdh|psgi|mkits)/|(?:(?:Makefile|Build)\.PL|README\.mdwn|LICENSE|MANIFEST|LOCALIZATION\.txt|Changes)$)

# Avoid temp and backup files.
~$
\.tmp$
\.old$
\.bak$
\..*?\.sw[po]$
\#$
\b\.#

# Avoid patch remnants
\.orig$
\.rej$

root/static/js/ckeditor/.*
root/git/.*
root/static/cgit/.*
root/static/.*\.zip$
root/uploads/.*
.gitignore
Maildir/*

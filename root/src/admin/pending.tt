[% # -*- mode: html -*- %]

<h3>[% c.loc('Pending revisions') %]</h3>

<form method="post">
  <table>
    [% FOREACH rev IN revisions %]
    <tr>
      <td>
        <a href="[% c.uri_for_action('/edit/edit', [rev.title.uri, rev.id]) %]">
          [% rev.title.uri %]
        </a>
      </td>
      <td>
        [% rev.updated.dmy %] [% rev.updated.hms %] [% c.loc('UTC') %]
      </td>
      <td>
        [% rev.status %]
      </td>
    </tr>
    [% END %]
  </table>
</form>
        
          
